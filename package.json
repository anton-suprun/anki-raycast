{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "anki",
  "title": "anki",
  "description": "Anki client for Raycast",
  "icon": "command-icon.png",
  "author": "absoluteunit1",
  "license": "MIT",
  "commands": [
    {
      "name": "decks",
      "title": "Decks",
      "subtitle": "Anki",
      "description": "Lists all Anki decks",
      "mode": "view"
    },
    {
      "name": "addCard",
      "title": "Add Card",
      "subtitle": "Anki",
      "description": "Add a card to a deck in Anki",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.0",
    "@types/showdown": "^2.0.6",
    "@types/turndown": "^5.0.4",
    "axios": "^1.5.1",
    "rss-parser": "^3.13.0",
    "showdown": "^2.1.0",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
